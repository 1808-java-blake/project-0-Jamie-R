package com.revature.beans;

public class Admin extends User {
	
	private boolean isAdmin;
	
	public Admin() {
		super();
	}
	
	public Admin(String username, String password, String name, double balance) {
		super(username, password, name, balance);
	}

	public Admin(boolean isAdmin, String username, String password, String name, double balance) {
		this(username, password, name, balance);
		this.isAdmin = isAdmin;
	}

	public Admin(boolean isAdmin) {
		super();
		this.isAdmin = isAdmin;
	}

	public boolean getIsAdmin() {
		return isAdmin;
	}

	public void setIsAdmin(boolean isAdmin) {
		this.isAdmin = isAdmin;
	}
	
	
}
